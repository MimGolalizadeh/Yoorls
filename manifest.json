{
    "manifest_version": 3,
    "name": "Yoorls",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "landing.html"
    }
  }

